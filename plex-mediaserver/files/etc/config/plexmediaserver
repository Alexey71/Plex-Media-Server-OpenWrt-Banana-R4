
config main
	option plex_drive_mnt '/mnt/ssd/PlexServer'
	option plex_browser_root '/mnt/ssd/PlexServer'
	option plex_library_dir '/mnt/ssd/PlexServer/.plex/Library'
	option plex_tmp_dir '/tmp/plexmediaserver'
	option plex_application_support_dir '/mnt/ssd/PlexServer/.plex/Library/Application Support'
	option plex_compressed_archive_path '/mnt/ssd/PlexServer/.plex/Library/Application/plexmediaserver.sqfs'
	option plex_bin_dir '/tmp/plexmediaserver/1.41.5.9522-a96edc606-aarch64'
	option plex_version '1.41.5.9522-a96edc606-aarch64'

# service plexmediaserver enable  включить автозапуск
# service plexmediaserver start   запустить сервер
# service plexmediaserver stop    остановить сервер
